<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title><%=h config.project %> Overview</title>

    <link href="stylesheets/screen.css" rel="stylesheet" type="text/css" media="screen" />
    <link href="stylesheets/print.css" rel="stylesheet" type="text/css" media="print" />
    <link rel="stylesheet" href="stylesheets/highlight.css">
    <script src="javascripts/highlight.pack.js"></script>
    <script>hljs.initHighlightingOnLoad();</script>

  </head>

  <body class="index">

    <div class="page-wrapper">
      <div class="content">
        <h1 id="introduction"><%=h config.project %> v<%= config.version %></h1>


	<%= unless Enum.empty?(modules) do %>
	<h2 id="modules_summary">Modules</h2>
	<%= for node <- modules, do: overview_entry_template(node, "Module") %>
	<% end %>

	<%= unless Enum.empty?(exceptions) do %>
	<h2 id="exceptions_summary">Exceptions</h2>
	<%= for node <- exceptions, do: overview_entry_template(node, "Exception") %>
	<% end %>

	<%= unless Enum.empty?(protocols) do %>
	<h2 id="protocols_summary">Protocols</h2>
	<%= for node <- protocols, do: overview_entry_template(node, "Protocol") %>
	<% end %>

	</div>
</div>
</body>
</html>
